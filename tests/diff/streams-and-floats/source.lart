include "types";
include "stdlib";
include "assert";

mod C {
  fn stdin() -> &FILE;
  fn stdout() -> &FILE;
  fn stderr() -> &FILE;
}

fn main() -> i32 {
  let stdin: &FILE = C::stdin();
  let x: f64 = 1.1;
  printf("you inserted: %f\n", x);
  assert(fscanf(stdin, "%lf", &x) == 1);
  printf("you inserted: %f\n", x);
  return 0;
}
